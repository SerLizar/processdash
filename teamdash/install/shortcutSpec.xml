<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>

<shortcuts>

  <shortcut
     name="Team Dashboard"
     programGroup="yes"
     desktop="yes"
     applications="no"
     startMenu="no"
     startup="no"
     target="javaw"
     commandLine="-Xmx${MAX_MEMORY}m -jar &quot;$INSTALL_PATH\pspdash.jar&quot; &quot;Team Dashboard&quot;"
     workingDirectory="$TEAM_DATA_PATH"
     description="Process Dashboard instance containing your team data"
     iconFile="$INSTALL_PATH\teamicon.ico"
     iconIndex="0"
     initialState="minimized">
       <createForPack id="teamTools"/>
  </shortcut>

  <shortcut
     name="Quick Launcher for Process Dashboard"
     programGroup="yes"
     desktop="yes"
     applications="no"
     startMenu="no"
     startup="no"
     target="javaw"
     commandLine="-DmaxMemory=${MAX_MEMORY}m -cp &quot;$INSTALL_PATH\pspdash.jar&quot; net.sourceforge.processdash.tool.quicklauncher.QuickLauncher"
     workingDirectory="$DATA_PATH"
     description="Launch Process Dashboard instances for data in ZIP files or arbitrary directories"
     iconFile="$INSTALL_PATH\launcher.ico"
     iconIndex="0"
     initialState="minimized">
       <createForPack id="teamTools"/>
  </shortcut>
   
</shortcuts>
