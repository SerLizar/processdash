<HTML>
<HEAD>
<link rel=stylesheet type="text/css" href="../style.css">
<TITLE><!--#echo var="[Process_ID]" --> Project Planning Summary</TITLE>
</HEAD>
<BODY>

<H1><!--#echo var="PATH_TRANSLATED" --></H1>
<H2><!--#echo var="[Process_ID]" --> Project Planning Summary</H2>

<FORM>

<INPUT TYPE=hidden NAME=requiredTag
<!--#if requiredTag -->
       VALUE="<!--#echo var=requiredTag -->"
<!--#elif rollup -->
       VALUE="<!--#echo var='rollup' --> Rollup Tag"
<!--#else-->
       VALUE="<!--#echo var="[Process_ID]" --> Tag"
<!--#endif-->
>

<!--#if not rollup -->

<TABLE>
<TR>
<TD>Project Owner</TD>
<TD><INPUT TYPE=text SIZE=50 NAME="/Owner	s"></TD>
</TR>

<TR>
<TD>Start Date</TD>
<TD><INPUT TYPE=text SIZE=25 NAME="Started	d"></TD>
</TR>

<TR>
<TD>Completion Date</TD>
<TD><INPUT TYPE=text SIZE=25 NAME="Completed	d"> Completed: <INPUT TYPE=checkbox NAME="Completed	d"></TD>
</TR>

<TR>
<TD>Keywords</TD>
<TD><INPUT TYPE=text SIZE=25 NAME="Project/Keywords	s"></TD>
</TR>

<TR>
<TD>Language</TD>
<TD><INPUT TYPE=text SIZE=15 NAME="Project/Language	s"></TD>
</TR>

</TABLE>
<!--#include file="../dash/selectRollup.class" -->

<!--#endif rollup -->



