<?xml version='1.0'?>

<file-list>

<file name="Coding Standard"
      path="[/{Project/Language} Coding Standard Document]"/>
 <var name="[/{Project/Language} Coding Standard Document]">
      Please enter the complete path to the document.  This document should
      already exist; the dashboard cannot create it automatically.</var>

<directory name="PSP Project Document Directory"
           templatePath="/Templates/pspDoc"
           path="[PSP Project Document Directory]">
      <var name="[PSP Project Document Directory]">
           Please enter the complete path to a directory where you would like
           to store documents pertaining to this PSP Project.</var>

  <file name="Requirements Statement" path="[Requirements Document Filename]"/>
  <file name="Requirements Document"  path="[Requirements Document Filename]"/>
   <var name="[Requirements Document Filename]">
        Please enter the path to this document (it need not reside in the
        PSP Project Document Directory listed above).  The document should
        already exist; the dashboard cannot create it automatically.</var>

  <file name="Conceptual Design" path="[Conceptual Design Filename]"
        defaultPath="Conceptual Design.rtf"
	templatePath="conceptualDesign.rtf"/>

  <file name="PIP Form" path="[PIP Form Filename]"
        defaultPath="Process Improvement Proposal.rtf" templatePath="C27.rtf"/>

  <file name="Test Report" path="[Test Report Filename]"
        defaultPath="Test Report.rtf" templatePath="C37.rtf"/>

  <file name="Design Review Checklist"
        path="[Design Review Checklist Filename]"
        defaultPath="Design Review Checklist.rtf"
	templatePath="[/{Project/Language} Design Review Checklist Template]"/>
   <var name="[/{Project/Language} Design Review Checklist Template]">
        Please enter the complete path to the document.  This document should
        already exist; the dashboard cannot create it automatically.</var>

  <file name="Code Review Checklist" path="[Code Review Checklist Filename]"
        defaultPath="Code Review Checklist.rtf"
	templatePath="[/{Project/Language} Code Review Checklist Template]"/>
   <var name="[/{Project/Language} Code Review Checklist Template]">
        Please enter the complete path to the document.  This document should
        already exist; the dashboard cannot create it automatically.</var>

  <file name="Operational Scenarios"
        path="[Operational Scenario Filename]"
	defaultPath="Operational Scenarios.rtf" templatePath="C66.rtf"/>

  <file name="Functional Specification"
        path="[Functional Specification Filename]"
        defaultPath="Functional Specification.rtf" templatePath="C68.rtf"/>

  <file name="State Specification"
        path="[State Specification Filename]"
        defaultPath="State Specification.rtf" templatePath="C70.rtf"/>

  <file name="Logic Specification"
        path="[Logic Specification Filename]"
        defaultPath="Logic Specification.rtf" templatePath="C72.rtf"/>

  <file name="Development Strategy" path="[Development Strategy Filename]"
        defaultPath="Development Strategy.rtf"
	templatePath="cycleStrategy.rtf"/>

  <file name="Issue Tracking Log"
        path="[Issue Tracking Log Filename]"
	defaultPath="Issue Tracking Log.rtf" templatePath="C85.rtf"/>

  <!-- add more lines here for additional files -->

</directory>

</file-list>
