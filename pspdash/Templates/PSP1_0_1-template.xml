<!--
   This process definition defines a PSP1.0.1 level process.  PSP1.0.1 was 
   defined at CMU for their MSE program.  It is basically just a PSP1 process    
   with added code and design reviews.
-->

<dashboard-process-template>

  <template name="PSP1.0.1" ID="PSP1.0.1" defectLog="true" usesRollup="PSP"
            dataFile="psp1.0.1/dataFile.txt" htmlID="top">
    <html ID="sum"  title="PSP1.0.1 Project Summary"    href="psp1.0.1/summary.htm"/>
    <html ID="size" title="PSP1.0.1 Size Estimating Template"
                    href="psp1.0.1/sizeest.class"/>
    <html ID="top"  title="PSP1.0.1 Process Script"     href="psp1.0.1/script.htm"/>
    <html ID="plan" title="PSP1.0.1 Planning Script"    href="psp1.0.1/planning.htm"/>
    <html ID="dev"  title="PSP1.0.1 Development Script" href="psp1.0.1/develop.htm"/>
    <html ID="pm"   title="PSP1.0.1 Postmortem Script"  href="psp1.0.1/postmort.htm"/>

    <phase name="Planning"      htmlID="plan" type="plan"/>
    <phase name="Design"        htmlID="dev"  type="dld"/>
    <phase name="Design Review" htmlID="dev"  type="dldr"/>
    <phase name="Code"          htmlID="dev"  type="code"/>
    <phase name="Code Review"   htmlID="dev"  type="cr"/>
    <phase name="Compile"       htmlID="dev"  type="comp"/>
    <phase name="Test"          htmlID="dev"  type="ut"/>
    <phase name="Postmortem"    htmlID="pm"   type="pm"/>
  </template>

</dashboard-process-template>
