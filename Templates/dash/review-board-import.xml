<defect-import-form>

    <textfield id="url"
        width="25"
        setting="url" />

    <review-board
        urlId="url"
	usernameId="username"
        passwordId="password" />

    <textfield id="username"
        setting="user.name" />

    <password id="password"
        setting="user.password" />

    <review-board-review-requests id="review_requests"
	usernameId="username" />

    <combobox id="review_request_id"
        choices="review_requests" />

    <textfield id="review_request_id"
        resourceId="review_request_id2"
        width="10"
        dataType="integer" />

    <default-inj-phase-selector />

    <default-rem-phase-selector />

    <review-board-defects id="defects"
        reviewRequestId="review_request_id" />

</defect-import-form>
