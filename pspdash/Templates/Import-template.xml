<dashboard-process-template>
  <template name="Import PSP Data" ID="Import PSP Data" htmlID="sum"
            usesRollup="PSP" dataFile="import/dataFile.txt" constraints="{">
    <html ID="sum" title="Import PSP Project Data" href="import/import.htm"/>

    <phase name="Planning"      imaginary="true" type="plan"/>
    <phase name="HLD"		imaginary="true" type="hld"/>
    <phase name="HLD Review"	imaginary="true" type="hldr"/>
    <phase name="Design"	imaginary="true" type="dld"/>
    <phase name="Design Review" imaginary="true" type="dldr"/>
    <phase name="Code"          imaginary="true" type="code"/>
    <phase name="Code Review"   imaginary="true" type="cr"/>
    <phase name="Compile"	imaginary="true" type="comp"/>
    <phase name="Test"		imaginary="true" type="ut"/>
    <phase name="Postmortem"	imaginary="true" type="pm"/>
    <phase name="Data Entry"	                 type="pm"/>
  </template>

</dashboard-process-template>
